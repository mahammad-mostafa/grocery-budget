<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= f.email_field :email, autocomplete: "email", placeholder: "Email (required)" %>
  <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password (required)" %>
  <%= f.submit "Login", id: "action" %>
<% end %>